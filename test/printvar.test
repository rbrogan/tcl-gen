package require tcltest
package require gen


::tcltest::test PrintVar-1 {Basic positive} -body {
set x 123
PrintVar x
} -result {} -output {x is 123
} -returnCodes {0}


::tcltest::test PrintVar-2 {No such variable} -body {
unset -nocomplain NoVar
PrintVar NoVar
} -result {Could not find variable NoVar in caller.} -output {} -returnCodes {1}


::tcltest::test PrintVar-3 {Empty string input} -body {
PrintVar ""
} -result {Variable name is missing. Got empty string.} -output {} -returnCodes {1}

