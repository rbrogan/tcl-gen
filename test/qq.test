package require tcltest
package require -exact gen 1.7.0

namespace eval QQTest {}



proc QQTest::Test-1 {} {

::tcltest::test QQ-0 {Basic test}  -constraints {}  -setup {sqlite3 testdb test.db}  -body {QQ "SELECT desc, notes FROM my_table WHERE id = 1"}  -cleanup {}  -result {uno one}  -output {}  -returnCodes {0}  

}



proc QQTest::Test-2 {} {

::tcltest::test QQ-1 {Empty query statement}  -constraints {}  -setup {}  -body {QQ ""
}  -cleanup {}  -result {Variable QueryStatement has empty value.}  -output {}  -returnCodes {1}  

}



if {([llength $argv] == 0) || ([lindex $argv 0] ne "target")} {
     QQTest::Test-1
     QQTest::Test-2
} else {
     foreach Element [lrange $argv 1 end] {
          QQTest::Test-[set Element]
     }
}

