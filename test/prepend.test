package require tcltest
package require gen


::tcltest::test Prepend-1 {Basic test} -body {
set MyString World
Prepend MyString "Hello "

} -result {Hello World} -output {} -returnCodes {0}


::tcltest::test Prepend-2 {String variable not pre-existing} -body {
unset -nocomplain NewVariable
Prepend NewVariable "abc"
puts $NewVariable
} -result {} -output {abc
} -returnCodes {0}


::tcltest::test Prepend-3 {Empty name for string variable} -body {
Prepend "" abc
} -result {Variable name is missing. Got empty string.} -output {} -returnCodes {1}


::tcltest::test Prepend-4 {Empty prepend value} -body {
set MyVar abc
Prepend MyVar ""
puts $MyVar

} -result {} -output {abc
} -returnCodes {0}

