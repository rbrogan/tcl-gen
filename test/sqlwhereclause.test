package require tcltest
package require -exact gen 1.7.1

namespace eval SqlWhereClauseTest {}



proc SqlWhereClauseTest::Test-1 {} {

::tcltest::test SqlWhereClause-0 {Basic test}  -constraints {}  -setup {}  -body {SqlWhereClause {intvalue 1}
}  -cleanup {}  -result {intvalue = 1}  -output {}  -returnCodes {0}  

}



proc SqlWhereClauseTest::Test-2 {} {

::tcltest::test SqlWhereClause-2 {Basic test, multiple values}  -constraints {}  -setup {}  -body {SqlWhereClause {intvalue 1 realvalue 2.3 textvalue 'four'}}  -cleanup {}  -result {intvalue = 1 AND realvalue = 2.3 AND textvalue = 'four'}  -output {}  -returnCodes {0}  

}



if {([llength $::argv] == 0) || ([lindex $::argv 0] ne "target")} {
     SqlWhereClauseTest::Test-1
     SqlWhereClauseTest::Test-2
} else {
     foreach Element [lrange $::argv 1 end] {
          SqlWhereClauseTest::Test-[set Element]
     }
}

